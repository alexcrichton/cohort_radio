<%= content_tag :h2, @parent.name if @parent %>

<ul class='alphabet pagination'>
	<li><%= link_to 'All', :songs %></li>
	<% ('A'..'Z').each do |letter| %>
		<li><%= link_to letter, songs_path + '?' + {:letter => letter}.to_query %></li>
	<% end %>
</ul>

<%= raw( paginated_section @songs do %>
	<%= render @songs %>
<% end ) %>
